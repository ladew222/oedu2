{
    "name": "imagex/openedu-project",
    "description": "Project template for Drupal 8 sites built with the OpenEDU distribution.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
    },
    "require": {
        "drupal-composer/drupal-scaffold": "^2.0.0",
        "cweagans/composer-patches": "^1.6.0",
        "imagex/openedu": "~8.3.0",
        "drupal/paragraphs": "^1.3",
        "drupal/workbench": "^1.0",
        "drupal/workbench_access": "^1.0",
        "drupal/packery": "^1.0",
        "drupal/crop": "^2.0",
        "drupal/color_field": "^2.0",
        "drupal/image_widget_crop": "^2.1",
        "drupal/weight": "3.x-dev",
        "drupal/devel": "^1.2",
        "drupal/twig_tweak": "^2.0",
        "drupal/chosen": "^2.5",
        "drupal/console": "~1.0",
        "drupal/migrate_plus": "^4.0",
        "drupal/migrate_tools": "^4.0",
        "drupal/migrate_source_csv": "^2.1",
        "drupal/views_autocomplete_filters": "^1.2",
        "drupal/libraries": "^3.0",
        "drupal/fullcalendar": "^1.0",
        "drupal/redirect": "^1.2",
        "drupal/linkit": "^4.3",
        "drupal/search_autocomplete": "^1.1",
        "drupal/blazy": "^1.0",
        "drupal/features": "^3.7",
        "drupal/ldap": "^3.0",
        "drupal/externalauth": "^1.1",
        "drupal/fontawesome": "^2.2",
        "drupal/views_bulk_edit": "^2.1",
        "drupal/ds": "^3.1",
        "drupal/radix_layouts": "^4.0",
        "drupal/csv_serialization": "^1.3",
        "drupal/vbo_export": "^2.0",
        "drupal/feeds": "^3.0",
        "drupal/migrate_upgrade": "^3.0",
        "drush/drush": "^9.3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
        ],
        "post-update-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
        ]
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "enable-patching": true
    }
}
